<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.chance.cs_399_calculator.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/buttonSubtract"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_marginBottom="85dp"
        android:layout_marginRight="0dp"
        android:text="-"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonAdd"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonSeven"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/buttonCLR"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="81dp"
        android:text="7"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonOne"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="255dp"
        android:text="1"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonTwo"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_marginLeft="90dp"
        android:layout_marginRight="180dp"
        android:layout_marginTop="255dp"
        android:text="2"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="@+id/buttonOne"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonMultiply"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="255dp"
        android:text="x"
        android:textSize="30sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonAdd"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="0dp"
        android:text="+"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonEquals"
        android:layout_width="269dp"
        android:layout_height="85dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="90dp"
        android:text="="
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonThree"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_marginRight="90dp"
        android:layout_marginTop="255dp"
        android:text="3"
        android:textSize="30sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonZero"
        android:layout_width="177dp"
        android:layout_height="83dp"
        android:layout_above="@+id/buttonEquals"
        android:layout_alignEnd="@+id/buttonEquals"
        android:layout_alignRight="@+id/buttonEquals"
        android:layout_marginBottom="85dp"
        android:layout_marginLeft="90dp"
        android:layout_marginRight="90dp"
        android:text="0"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/buttonDot"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/buttonSeven"
        android:layout_marginBottom="85dp"
        android:layout_marginLeft="0dp"
        android:text="."
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/buttonDivide"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_alignLeft="@+id/buttonSix"
        android:layout_alignStart="@+id/buttonSix"
        android:layout_below="@+id/buttonCLR"
        android:layout_marginRight="0dp"
        android:layout_marginTop="168dp"
        android:text="/"
        android:textSize="30sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonNine"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_above="@+id/buttonSix"
        android:layout_alignEnd="@+id/buttonCLR"
        android:layout_alignRight="@+id/buttonCLR"
        android:layout_marginLeft="182dp"
        android:layout_marginRight="91dp"
        android:layout_marginTop="81dp"
        android:text="9"
        android:textSize="30sp"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintLeft_toLeftOf="@+id/buttonSeven"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonFour"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_alignLeft="@+id/buttonZero"
        android:layout_alignStart="@+id/buttonZero"
        android:layout_below="@+id/buttonZero"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="168dp"
        android:text="4"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonFive"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_alignBaseline="@+id/buttonSix"
        android:layout_alignBottom="@+id/buttonSix"
        android:layout_marginLeft="91dp"
        android:layout_marginRight="181dp"
        android:layout_marginTop="168dp"
        android:layout_toEndOf="@+id/buttonZero"
        android:layout_toRightOf="@+id/buttonZero"
        android:text="5"
        android:textSize="30sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/buttonFour"
        app:layout_constraintRight_toRightOf="@+id/buttonDivide"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonSix"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_alignTop="@+id/buttonZero"
        android:layout_marginLeft="182dp"
        android:layout_marginRight="91dp"
        android:layout_marginTop="168dp"
        android:layout_toLeftOf="@+id/buttonZero"
        android:layout_toStartOf="@+id/buttonZero"
        android:text="6"
        android:textSize="30sp"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintLeft_toLeftOf="@+id/buttonFour"
        app:layout_constraintRight_toRightOf="@+id/buttonDivide"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonEight"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_below="@+id/buttonEquals"
        android:layout_marginLeft="91dp"
        android:layout_marginRight="182dp"
        android:layout_marginTop="81dp"
        android:layout_toEndOf="@+id/buttonSubtract"
        android:layout_toRightOf="@+id/buttonSubtract"
        android:text="8"
        android:textSize="30sp"
        app:layout_constraintHorizontal_bias="0.285"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/buttonCLR"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonCLR"
        android:layout_width="90dp"
        android:layout_height="85dp"
        android:layout_marginLeft="271dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="82dp"
        android:text="CLR"
        android:textSize="30sp"
        app:layout_constraintLeft_toLeftOf="@+id/buttonSeven"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textAns"
        android:layout_width="344dp"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="41dp"
        android:text=""
        android:textSize="30sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textCalc"
        android:layout_width="344dp"
        android:layout_height="39dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text=""
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>
